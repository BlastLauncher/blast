{
  "name": "@blastlauncher/runtime",
  "version": "1.1.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "pnpm run clean && tsc",
    "clean": "rimraf dist/",
    "build:esbuild": "pnpm run clean && node build.js",
    "build:watch": "pnpm run clean && tsc -w",
    "start:dev": "nodemon --exec node dist/index.js",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@blastlauncher/api": "workspace:*",
    "@blastlauncher/renderer": "workspace:*",
    "@raycast/api": "workspace:@blastlauncher/api@*",
    "@raycast/utils": "^1.5.2",
    "eval": "^0.1.8",
    "node-fetch": "^2.6.8",
    "react": "^18.2.0",
    "react-devtools-core": "^4.27.1",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@types/node-fetch": "2",
    "@types/react": "18.0.9",
    "@types/websocket": "^1.0.5",
    "esbuild": "^0.17.5",
    "raycast-original": "npm:@raycast/api@*",
    "rimraf": "^4.1.1",
    "rpc-websockets": "^7.5.0"
  },
  "repository": "https://github.com/BlastLauncher/blast.git"
}
